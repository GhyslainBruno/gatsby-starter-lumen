{
  "version": "3.0.0",
  "name": "david-wells-io",
  "description": "Davids site",
  "homepage": "https://davidwells.io",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "http-server public",
    "deploy": "netlify deploy -p --dir public",
    "postbuild": "cp -R resume public && node scripts/sitemap.js",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'"
  },
  "repository": "git+https://github.com/davidwells/davidwells.io.git",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "theme",
    "starter"
  ],
  "author": "David Wells <hello@davidwells.io>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "clipboard": "^1.7.1",
    "date-fns": "^1.30.1",
    "gatsby-remark-autolink-headers": "^2.2.4",
    "gatsby-remark-external-links": "0.0.4",
    "get-form-data": "^2.0.0",
    "lodash": "^4.17.15",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.20.35",
    "gatsby-plugin-catch-links": "^2.2.4",
    "gatsby-plugin-feed": "2.4.4",
    "gatsby-plugin-google-analytics": "^2.2.5",
    "gatsby-plugin-offline": "^3.1.5",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-remark-copy-linked-files": "^2.2.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-remark-prismjs": "^3.4.5",
    "gatsby-remark-responsive-iframe": "^2.3.4",
    "gatsby-remark-smartypants": "^2.2.4",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "icon-pipeline": "0.0.5",
    "markdown-magic": "^1.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^2.0.5",
    "sitemap": "^1.13.0"
  }
}
